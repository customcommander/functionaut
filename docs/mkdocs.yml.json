{
  "site_name": "Functionaut",
  "site_url": "https://customcommander.github.io/functionaut/",
  "repo_url": "https://github.com/customcommander/functionaut",
  "edit_uri": "",
  "theme": {
    "name": "material",
    "icon": {
      "repo": "fontawesome/brands/github"
    },
    "features": [
      "navigation.tabs",
      "navigation.tabs.sticky",
      "content.code.annotate"
    ]
  },
  "markdown_extensions": [
    "def_list",
    "tables",
    "admonition",
    "pymdownx.details",
    "pymdownx.superfences",
    "pymdownx.highlight",
    "pymdownx.inlinehilite",
    "pymdownx.snippets"
  ],
  "nav": [
    {
      "Home": [
        {
          "README": "README.md"
        },
        {
          "Contribute": "CONTRIBUTING.md"
        }
      ]
    },
    {
      "Glossary": "manual/glossary.md"
    },
    {
      "Cookbook": "manual/cookbook.md"
    }
  ]
}